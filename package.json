{
  "name": "louxor",
  "version": "1.1.0",
  "description": "A web player frontend for MPD",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "npm run serve",
    "serve": "DEBUG=mpc,io,express node index.js",
    "prebuild": "rm -rf build && mkdir build",
    "build": "NODE_ENV=production npm-run-all -p build:*",
    "build:css": "stylus src/client/css/style.css -o build",
    "build:html": "cp src/client/index.html build",
    "build:images": "cp -r src/client/images build",
    "build:js": "browserify -t babelify src/client/js/index.js -o build/bundle.js",
    "postbuild:js": "uglifyjs build/bundle.js -o build/bundle.js",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "npm run build:css -- --watch",
    "watch:js": "watchify -t babelify -o build/bundle.js src/client/js/index.js"
  },
  "keywords": [
    "mpd",
    "music",
    "react"
  ],
  "author": "Delapouite <delapouite@gmail.com> (http://delapouite.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/delapouite/louxor.git"
  },
  "bugs": {
    "url": "https://github.com/delapouite/louxor/issues"
  },
  "homepage": "https://github.com/delapouite/louxor",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.3",
    "classnames": "^2.2.5",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "mpcpp": "^1.3.0",
    "react": "^15.3.1",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "screenfull": "^3.0.2",
    "socket.io": "^1.4.8",
    "stylus": "^0.54.5",
    "trumpet": "^1.7.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "npm-run-all": "^3.0.0",
    "stylus": "^0.54.5",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  }
}
